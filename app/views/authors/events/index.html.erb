<% provide(:page_title, "All Events") %>
<% provide(:author_events, "active") %>

<div class="row">
  <%= render 'authors/navigation/navigation_bar' %>
  <div class="col-md-8">
    <div>
      <%= link_to 'New Event', new_authors_event_path, class: 'btn btn-primary' %>
    </div>
    <br>
    <br>
    <table class="table table-striped">
      <tr>
        <th colspan="5">Event Title</th>
      </tr>
      <%= render @events %>
    </table>
    <div class="apple_pagination">
      <%= will_paginate @events, container: false %>
    </div>
  </div>
</div>
